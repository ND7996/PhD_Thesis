!new opls parameters and substrate parameters (ffld?)
readlib  qoplsaa.lib          
readlib  GPX.lib

! parameters
readprm  qoplsaa_all1.prm

! read in structure
readpdb intermediate_nohyd.pdb

set solvent_pack 2.7

! Solvate around C49
boundary   sphere 49:SE1 25.
solvate           49:SE1 25. grid HOH

! write out topology and pdb with corresponding indices
maketop GPX6sechuman.top
writetop GPX6sechuman.top
writepdb GPX6sechuman.pdb y

quit
