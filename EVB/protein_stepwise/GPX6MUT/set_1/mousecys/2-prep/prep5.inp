!new opls parameters and substrate parameters (ffld?)
readlib  qoplsaa.lib          
readlib  GPX.lib

! parameters
readprm  qoplsaa_all2.prm

! read in structure

readpdb intermediate_nohyd.pdb

set solvent_pack 2.7

! Solvate around C49

boundary   sphere 49:SG 25.
solvate           49:SG 25. grid HOH

! write out topology and pdb with corresponding indices
maketop GPX6cysmouse.top
writetop GPX6cysmouse.top
writepdb GPX6cysmouse.pdb y
quit
